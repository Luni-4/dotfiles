# Some more ls aliases
alias ls='ls --color=auto'
alias la='ls -al'

# Grep alias
alias grep='grep --color=auto'

# Vim alias
alias vim='nvim'

# Update and upgrade aliases
alias ur='sudo apt dist-upgrade'
alias i='sudo apt install'

# Git aliases
alias ga='git add'
alias gc='git commit'
alias rb2='git rebase -i HEAD~2'
alias pru='git prune origin'

# Tmux aliases
alias dev='~/scripts/dev-tmux'
alias job='~/scripts/job-tmux ~/vlc job'

# Python alias
alias python='python3.7'
